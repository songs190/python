def f_to_c(f_temp):
  c_temp = (f_temp - 32) * 5/9
  return c_temp

def c_to_f(c_temp):
  f_temp = c_temp * (9/5) + 32
  return f_temp
print(c_to_f(0))

def get_energy(mass, c):
    c = 3 * pow(10, 8)
    return mass * c * c
print(get_energy(3, 3))

def introduction(first_name, last_name):
  return last_name + ", " + first_name + " " + last_name

print(introduction("Jason", "Song"))


#Bond, James Bond

def square_root(x):
    return x ** 0.5
print(square_root(16))
# ** 0.5 is the square root
