my_baby_bool = "true"
print(type(my_baby_bool))

my_baby_bool_two = True
print(type(my_baby_bool_two))
# boolean is always TRUE or FALSE without "" Top one is string


def wake_up(day):
    if day == "Monday":
        return "Wake up at 6AM."
    if day == "Tuesday":
        return "Wake up at 6AM."
    if day == "Wednesday":
        return "Wake up at 6AM."
    if day == "Thursday":
        return "Wake up at 6AM."
    if day == "Friday":
        return "Wake up at 6AM."
    if day == "Saturday":
        return "Sleep in."
    if day == "Sunday":
        return "Sleep in."
    else:
        return "insert day"

print(wake_up("Sunday"))
print(wake_up("yoooo"))
print(wake_up("Monday"))

day = "Monday"
print(wake_up(day))
day = "Sunday"
print(wake_up(day))

def greater_than(x, y):
  if x == y:
    return "These numbers are the same"
  if x >= y:
    return "x is equal or greater than y"
  if x <= y:
    return "x is equal or less than y"
print(greater_than(3, 2))

def graduation_reqs(credits):
  if credits >= 120:
    return "You have enough credits to graduate!"
  else:
    return "You don't have enough credits to graduate!"
print(graduation_reqs(120))



def graduation_reqs(gpa, credits):
    if (gpa >= 2.0) and (credits >= 120):
        return "You meet the requirements to graduate!"


def graduation_reqs(gpa, credits):
  if (gpa >= 2.0) and (credits >= 120):
    return "You meet the requirements to graduate!"
  if (gpa >= 2.0) and not (credits >= 120):
    return "You do not have enough credits to graduate."
  if not (gpa >= 2.0) and (credits >= 120):
    return "Your GPA is not high enough to graduate."
  if not (gpa >= 2.0) and not (credits >= 120):
    return "You do not meet either requirement to graduate!"
# Using not & and operators

def grade_converter(gpa):
    grade = "F"

    if gpa >= 4.0:
        grade = "A"
    elif gpa >= 3.0:
        grade = "B"
    elif gpa >= 2.0:
        grade = "C"
    elif gpa >= 1.0:
        grade = "D"

    return grade
